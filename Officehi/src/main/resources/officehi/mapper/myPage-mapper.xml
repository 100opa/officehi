<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.groupware.officehi.repository.MyPageRepository">

	<select id="findByUserNo" resultType="MyPageDTO">
		select * 
		from user
		where user_no = #{userNo}
	</select>
	
</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.groupware.officehi.repository.NoticeRepository">

	<select id="findByAll" resultType="com.groupware.officehi.dto.Notice">
		select *
		from notice
	</select>

	<select id="findById" parameterType="Long" resultType="com.groupware.officehi.dto.Notice">
		select *
		from notice
		where notice_no=#{notice_no}
	</select>

</mapper>